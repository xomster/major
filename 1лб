def check_positive():
    try:
        # Ввод трёх целых чисел A, B, C
        A = int(input("Введите число A: "))
        B = int(input("Введите число B: "))
        C = int(input("Введите число C: "))
    except ValueError:
        print("Все значения должны быть целыми числами!")
        return
    
    # Проверка условия: хотя бы одно из чисел положительное
    res = A > 0 or B > 0 or C > 0
    
    # Вывод результата
    print( res)

# Вызов функции для проверки
check_positive()
